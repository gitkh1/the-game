{
  "name": "mongo",
  "version": "1.0.0",
  "scripts": {
    "build": "docker build . -t mongo_db",
    "remove-image": "docker image rm mongo_db",
    "start": "docker run -p27017:27017 -d --env-file ../../.env --rm --name mongo_container mongo_db",
    "stop": "docker stop mongo_container"
